{
  "name": "@geneecode/geneeui",
  "version": "0.0.4",
  "description": "GeneeUI is a React based component library, providing everyday use components for building web applications",
  "main": "lib/index.js",
  "module": "lib/esm/index.js",
  "esnext": "lib/es/index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage && opener ./coverage/lcov-report/index.html",
    "clean": "rimraf lib && rimraf dist",
    "prebuild": "npm run clean",
    "build": "npm run build:cjs && npm run build:esm && npm run build:es && npm run build:umd && npm run build:styles-raw && npm run build:styles",
    "build:cjs": "tsc",
    "build:esm": "tsc -m es2015 --outDir lib/esm",
    "build:es": "tsc -m es2015 -t es2018 --outDir lib/es",
    "build:umd": "rollup -c",
    "build:styles-raw": "scss-bundle",
    "build:styles": "npm run build:styles:dev && npm run build:styles:prod",
    "build:styles:dev": "node-sass src/styles/geneeui.scss dist/geneeui.css",
    "build:styles:prod": "node-sass src/styles/geneeui.scss dist/geneeui.min.css --output-style compressed",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "create:component": "ts-node scripts/create-component.ts",
    "prepublishOnly": "npm test && npm run lint && npm run build",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "eslint  --ext .tsx,.ts ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/geneecode/geneeui.git"
  },
  "keywords": [
    "geneeui",
    "geneecode",
    "storybook",
    "react",
    "typescript",
    "dom",
    "componentlib",
    "scss",
    "css",
    "grid",
    "ui",
    "web",
    "components"
  ],
  "author": "geneecode.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/geneecode/geneeui/issues"
  },
  "homepage": "https://github.com/geneecode/geneeui#readme",
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/addon-docs": "^5.2.8",
    "@storybook/addon-knobs": "^5.2.8",
    "@storybook/addon-storyshots": "^5.2.8",
    "@storybook/addon-storysource": "^5.2.8",
    "@storybook/addons": "^5.2.8",
    "@storybook/preset-scss": "^1.0.2",
    "@storybook/preset-typescript": "^1.2.0",
    "@storybook/react": "^5.2.8",
    "@storybook/source-loader": "^5.2.8",
    "@storybook/theming": "^5.2.8",
    "@types/jest": "^24.0.16",
    "@types/node": "^12.6.9",
    "@types/react": "^16.8.24",
    "@types/react-dom": "^16.8.5",
    "@types/react-select": "^3.0.4",
    "@types/storybook__addon-storyshots": "^5.1.1",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "change-case": "^3.1.0",
    "css-loader": "^3.2.1",
    "eslint": "^6.1.0",
    "eslint-plugin-react": "^7.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.8.0",
    "node-sass": "^4.12.0",
    "opener": "^1.5.1",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-docgen-typescript-loader": "^3.6.0",
    "react-dom": "^16.8.6",
    "react-test-renderer": "^16.8.6",
    "require-context.macro": "^1.1.1",
    "rimraf": "^2.6.3",
    "rollup": "^1.18.0",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-size-snapshot": "^0.10.0",
    "rollup-plugin-terser": "^5.1.3",
    "rollup-plugin-typescript2": "^0.22.1",
    "sass-loader": "^7.3.1",
    "scss-bundle": "^3.0.1",
    "style-loader": "^0.23.1",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.3.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.3"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "ag-grid-community": "^21.2.2",
    "ag-grid-react": "^21.2.2"
  },
  "dependencies": {
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@storybook/addon-options": "^5.2.6",
    "@types/react-color": "^3.0.1",
    "@types/react-flags-select": "^1.1.2",
    "@types/react-text-mask": "^5.4.6",
    "@types/storybook__react": "^4.0.2",
    "@wojtekmaj/react-datetimerange-picker": "^2.4.0",
    "ag-grid-community": "^21.2.2",
    "ag-grid-react": "^21.2.2",
    "clone": "^2.1.2",
    "clsx": "^1.0.4",
    "material-design-icons": "^3.0.1",
    "moment": "^2.24.0",
    "react-color": "^2.17.3",
    "react-d3-tree": "^1.15.3",
    "react-datetime": "^2.16.3",
    "react-dropzone": "^10.1.10",
    "react-google-charts": "^3.0.15",
    "react-phone-input-2": "^2.10.1",
    "react-phone-number-input": "^2.4.1",
    "react-popover": "^0.5.10",
    "react-select": "^3.0.4",
    "react-switch": "^5.0.1",
    "react-text-mask": "^5.4.3",
    "react-tiny-popover": "^3.4.5"
  }
}
